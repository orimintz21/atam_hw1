.global _start

.section .text
    mov $root, %rax
    cmpq $node1, (%rax)
    jne bad_exit

    movq $node1, %rax
    cmpq $1, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $node4, 16(%rax)
    jne bad_exit

    movq $node4, %rax
    cmpq $4, (%rax)
    jne bad_exit
    cmpq $node2, 8(%rax)
    jne bad_exit
    cmpq $new_node, 16(%rax)
    jne bad_exit

    movq $node2, %rax
    cmpq $2, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $new_node, %rax
    cmpq $7, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node1
node1:
  .quad 1
  .quad 0
  .quad node4
node4:
  .quad 4
  .quad node2
  .quad 0
node2:
  .quad 2
  .quad 0
  .quad 0
new_node: .quad 7, 0, 0
