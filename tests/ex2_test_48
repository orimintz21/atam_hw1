.global _start

.section .text
    mov $destination, %rax
    cmpb $176, 0(%rax)
    jne bad_exit

    cmpb $229, 1(%rax)
    jne bad_exit

    cmpb $35, 2(%rax)
    jne bad_exit

    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
num: .int 3
source: .byte 176,229,35,209,159,227,166,174,169,200,251,160,84,41,208,233,132,24,189,88,191,42,102,37,221,122,150,95,22,157,32,151,230,144,58,240,153,180,66,161,229
destination: .zero 64
