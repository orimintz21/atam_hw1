.global _start

.section .text
    mov $root, %rax
    cmpq $node4, (%rax)
    jne bad_exit

    movq $node4, %rax
    cmpq $4, (%rax)
    jne bad_exit
    cmpq $node0, 8(%rax)
    jne bad_exit
    cmpq $node6, 16(%rax)
    jne bad_exit

    movq $node0, %rax
    cmpq $0, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $node2, 16(%rax)
    jne bad_exit

    movq $node6, %rax
    cmpq $6, (%rax)
    jne bad_exit
    cmpq $node5, 8(%rax)
    jne bad_exit
    cmpq $node9, 16(%rax)
    jne bad_exit

    movq $node2, %rax
    cmpq $2, (%rax)
    jne bad_exit
    cmpq $node1, 8(%rax)
    jne bad_exit
    cmpq $node3, 16(%rax)
    jne bad_exit

    movq $node5, %rax
    cmpq $5, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node9, %rax
    cmpq $9, (%rax)
    jne bad_exit
    cmpq $node7, 8(%rax)
    jne bad_exit
    cmpq $node14, 16(%rax)
    jne bad_exit

    movq $node1, %rax
    cmpq $1, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node3, %rax
    cmpq $3, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node7, %rax
    cmpq $7, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node14, %rax
    cmpq $14, (%rax)
    jne bad_exit
    cmpq $new_node, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $new_node, %rax
    cmpq $12, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node4
node4:
  .quad 4
  .quad node0
  .quad node6
node0:
  .quad 0
  .quad 0
  .quad node2
node6:
  .quad 6
  .quad node5
  .quad node9
node2:
  .quad 2
  .quad node1
  .quad node3
node5:
  .quad 5
  .quad 0
  .quad 0
node9:
  .quad 9
  .quad node7
  .quad node14
node1:
  .quad 1
  .quad 0
  .quad 0
node3:
  .quad 3
  .quad 0
  .quad 0
node7:
  .quad 7
  .quad 0
  .quad 0
node14:
  .quad 14
  .quad 0
  .quad 0
new_node: .quad 12, 0, 0
