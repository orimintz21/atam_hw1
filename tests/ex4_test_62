.global _start

.section .text
    mov $root, %rax
    cmpq $node10, (%rax)
    jne bad_exit

    movq $node10, %rax
    cmpq $10, (%rax)
    jne bad_exit
    cmpq $node2, 8(%rax)
    jne bad_exit
    cmpq $node14, 16(%rax)
    jne bad_exit

    movq $node2, %rax
    cmpq $2, (%rax)
    jne bad_exit
    cmpq $node1, 8(%rax)
    jne bad_exit
    cmpq $node7, 16(%rax)
    jne bad_exit

    movq $node14, %rax
    cmpq $14, (%rax)
    jne bad_exit
    cmpq $node11, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node1, %rax
    cmpq $1, (%rax)
    jne bad_exit
    cmpq $node0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node7, %rax
    cmpq $7, (%rax)
    jne bad_exit
    cmpq $new_node, 8(%rax)
    jne bad_exit
    cmpq $node8, 16(%rax)
    jne bad_exit

    movq $node11, %rax
    cmpq $11, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $node13, 16(%rax)
    jne bad_exit

    movq $node0, %rax
    cmpq $0, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $new_node, %rax
    cmpq $3, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node8, %rax
    cmpq $8, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node13, %rax
    cmpq $13, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node10
node10:
  .quad 10
  .quad node2
  .quad node14
node2:
  .quad 2
  .quad node1
  .quad node7
node14:
  .quad 14
  .quad node11
  .quad 0
node1:
  .quad 1
  .quad node0
  .quad 0
node7:
  .quad 7
  .quad 0
  .quad node8
node11:
  .quad 11
  .quad 0
  .quad node13
node0:
  .quad 0
  .quad 0
  .quad 0
node8:
  .quad 8
  .quad 0
  .quad 0
node13:
  .quad 13
  .quad 0
  .quad 0
new_node: .quad 3, 0, 0
