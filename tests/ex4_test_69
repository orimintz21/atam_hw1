.global _start

.section .text
    mov $root, %rax
    cmpq $node9, (%rax)
    jne bad_exit

    movq $node9, %rax
    cmpq $9, (%rax)
    jne bad_exit
    cmpq $node8, 8(%rax)
    jne bad_exit
    cmpq $node10, 16(%rax)
    jne bad_exit

    movq $node8, %rax
    cmpq $8, (%rax)
    jne bad_exit
    cmpq $node5, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node10, %rax
    cmpq $10, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $node12, 16(%rax)
    jne bad_exit

    movq $node5, %rax
    cmpq $5, (%rax)
    jne bad_exit
    cmpq $node3, 8(%rax)
    jne bad_exit
    cmpq $node7, 16(%rax)
    jne bad_exit

    movq $node12, %rax
    cmpq $12, (%rax)
    jne bad_exit
    cmpq $node11, 8(%rax)
    jne bad_exit
    cmpq $node13, 16(%rax)
    jne bad_exit

    movq $node3, %rax
    cmpq $3, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $new_node, 16(%rax)
    jne bad_exit

    movq $node7, %rax
    cmpq $7, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node11, %rax
    cmpq $11, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node13, %rax
    cmpq $13, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $new_node, %rax
    cmpq $4, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node9
node9:
  .quad 9
  .quad node8
  .quad node10
node8:
  .quad 8
  .quad node5
  .quad 0
node10:
  .quad 10
  .quad 0
  .quad node12
node5:
  .quad 5
  .quad node3
  .quad node7
node12:
  .quad 12
  .quad node11
  .quad node13
node3:
  .quad 3
  .quad 0
  .quad 0
node7:
  .quad 7
  .quad 0
  .quad 0
node11:
  .quad 11
  .quad 0
  .quad 0
node13:
  .quad 13
  .quad 0
  .quad 0
new_node: .quad 4, 0, 0
