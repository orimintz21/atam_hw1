.global _start

.section .text
    mov $root, %rax
    cmpq $node9, (%rax)
    jne bad_exit

    movq $node9, %rax
    cmpq $9, (%rax)
    jne bad_exit
    cmpq $node5, 8(%rax)
    jne bad_exit
    cmpq $node11, 16(%rax)
    jne bad_exit

    movq $node5, %rax
    cmpq $5, (%rax)
    jne bad_exit
    cmpq $node2, 8(%rax)
    jne bad_exit
    cmpq $node6, 16(%rax)
    jne bad_exit

    movq $node11, %rax
    cmpq $11, (%rax)
    jne bad_exit
    cmpq $node10, 8(%rax)
    jne bad_exit
    cmpq $node12, 16(%rax)
    jne bad_exit

    movq $node2, %rax
    cmpq $2, (%rax)
    jne bad_exit
    cmpq $node1, 8(%rax)
    jne bad_exit
    cmpq $node3, 16(%rax)
    jne bad_exit

    movq $node6, %rax
    cmpq $6, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $node8, 16(%rax)
    jne bad_exit

    movq $node10, %rax
    cmpq $10, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node12, %rax
    cmpq $12, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $node13, 16(%rax)
    jne bad_exit

    movq $node1, %rax
    cmpq $1, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node3, %rax
    cmpq $3, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node8, %rax
    cmpq $8, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $node13, %rax
    cmpq $13, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $new_node, 16(%rax)
    jne bad_exit

    movq $new_node, %rax
    cmpq $16, (%rax)
    jne bad_exit
    cmpq $0, 8(%rax)
    jne bad_exit
    cmpq $0, 16(%rax)
    jne bad_exit

    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node9
node9:
  .quad 9
  .quad node5
  .quad node11
node5:
  .quad 5
  .quad node2
  .quad node6
node11:
  .quad 11
  .quad node10
  .quad node12
node2:
  .quad 2
  .quad node1
  .quad node3
node6:
  .quad 6
  .quad 0
  .quad node8
node10:
  .quad 10
  .quad 0
  .quad 0
node12:
  .quad 12
  .quad 0
  .quad node13
node1:
  .quad 1
  .quad 0
  .quad 0
node3:
  .quad 3
  .quad 0
  .quad 0
node8:
  .quad 8
  .quad 0
  .quad 0
node13:
  .quad 13
  .quad 0
  .quad 0
new_node: .quad 16, 0, 0
